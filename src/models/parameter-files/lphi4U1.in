tOutputRareFreq = 10
tMax = 100

#GWs
withGWs = true

#IC
kCutOff = 3
initial_amplitudes = 0
initial_momenta = 0
cmplx_field_initial_norm = 5.6964e18     # homogeneous amplitudes in GeV
cmplx_momentum_initial_norm = -6.88343e30 # homogeneous amplitudes in GeV2

#Gauge couplings and effectiveCharges
CSU1Charges = 1
gU1s = 1.11037e-05

#Model Parameters
lambda = 9e-14